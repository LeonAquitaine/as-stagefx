      "\\\\build\\\\",
      "\\\\config\\\\",
      "\\\\docs\\\\"
    ]
  },  "fileExtensions": {
    "shaders": [".fx", ".fxh"]
  },  "buildRules": {
    "excludePatterns": ["^\\[PRE\\]"],
