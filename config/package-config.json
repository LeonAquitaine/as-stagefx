      "globalDependencies": ["AS_Utils.1.fxh", "AS_Noise.1.fxh"],
      "includePatterns": ["#include\\s+[\"<]([^\">]+)[\">]"],
      "texturePatterns": ["texture\\s+\\w+\\s*<\\s*source\\s*=\\s*([^;>]+)\\s*[;>]"]
    }
  },  "dynamicPackages": {
    "enabled": true,
    "autoDiscovery": {
      "complete": {
